# Frontend Environment Configuration Template
# Copy this file to .env and fill in your actual values

# React Configuration
REACT_APP_PORT=3000

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8080/api/v1/

# MQTT Configuration
REACT_APP_MQTT_URL=broker.emqx.io
REACT_APP_MQTT_PROTOCOL=wss
REACT_APP_MQTT_PATH=mqtt
REACT_APP_MQTT_PORT=8084

# Keycloak Configuration
REACT_APP_KEYCLOAK_URL=https://auth.2serv.eu/auth
REACT_APP_KEYCLOAK_REALM=master
REACT_APP_KEYCLOAK_CLIENT_ID=YOUR_CLIENT_ID

# Environment
REACT_APP_STAGING=false
